{
  "name": "git-viz-vscode",
  "displayName": "Git Viz",
  "description": "Horizontal Git repository visualization for VS Code and Cursor",
  "version": "3.5.5",
  "publisher": "kambei",
  "icon": "media/main-icon.png",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:git-viz.open",
    "onCommand:git-viz.refresh",
    "onCommand:git-viz.zoomIn",
    "onCommand:git-viz.zoomOut"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "iconThemes": [
      {
        "id": "git-viz-icons",
        "label": "Git Viz Icons",
        "path": "./media/icons.json"
      }
    ],
    "commands": [
      {
        "command": "git-viz.open",
        "title": "Open Git Viz",
        "category": "Git Viz"
      },
      {
        "command": "git-viz.refresh",
        "title": "Refresh",
        "category": "Git Viz"
      },
      {
        "command": "git-viz.zoomIn",
        "title": "Zoom In",
        "category": "Git Viz"
      },
      {
        "command": "git-viz.zoomOut",
        "title": "Zoom Out",
        "category": "Git Viz"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceScheme == file",
          "command": "git-viz.open",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "command": "git-viz.open",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "git-viz.refresh",
          "when": "git-viz.active"
        },
        {
          "command": "git-viz.zoomIn",
          "when": "git-viz.active"
        },
        {
          "command": "git-viz.zoomOut",
          "when": "git-viz.active"
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kambei/git-viz-vscode.git"
  },
  "keywords": [
    "git",
    "visualization",
    "vscode",
    "extension",
    "horizontal",
    "graph",
    "kambei"
  ],
  "author": "kambei",
  "license": "MIT",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "package": "vsce package"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@types/node": "12.x",
    "@types/vscode": "^1.40.0",
    "babel-loader": "^8.0.0",
    "vsce": "^2.15.0",
    "webpack": "^5.102.1",
    "webpack-cli": "^4.0.0"
  }
}
